# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/neo"

# Server
PORT=5000
NODE_ENV=development
CLIENT_URL=http://localhost:3000

# JWT
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here

# Qwen-Omni Voice Call Configuration
# Get your API key from: https://dashscope.console.aliyun.com/
DASHSCOPE_API_KEY=your-dashscope-api-key-here
REALTIME_MODEL=qwen3-omni-flash-realtime
REALTIME_BASE=wss://dashscope.aliyuncs.com/api-ws/v1/realtime

# Optional: HTTP Proxy Configuration (if needed to access Qwen-Omni API)
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=http://proxy.example.com:8080

# Logging
LOG_LEVEL=info
